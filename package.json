{
  "name": "keyu",
  "version": "1.4.2",
  "description": "Key utilities for everyday use",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/standard/bin/cmd.js ./ && ./node_modules/.bin/eslint --fix ./",
    "ci": "NODE_ENV=test ./node_modules/.bin/eslint --fix ./ && nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test": "NODE_ENV=test mocha",
    "coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text npm test"
  },
  "keywords": [
    "utils",
    "utilities",
    "key"
  ],
  "author": "Carles Iborra",
  "repository": "github:nerac/keyu",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0",
    "standard": "^12.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm test",
      "pretty-quick --staged"
    ]
  }
}
